        <Setter Property="ToolTipService.InitialShowDelay" Value="1000"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="SBackground" Value="{StaticResource VSBackground}"/>
        <Setter Property="SForeground" Value="White"/>
    </Style>
